<!-- الكلية التطبيقية   -->
<!-- الكلية التطبيقية   -->
<script>
  $(document).ready(function() {
    // Display/hide colleges
    $("#colleges-btn").click(function() {
      $("#colleges1").toggle();
    });
    // Display/hide college 2 majors
    $("#college2-btn").click(function() {
      $("#college2-majors").toggle();
    });

    // Display/hide college 2 major 2 sections
    $("#college2-major2-btn").click(function() {
      $("#college2-major2-sections").toggle();
    });
    // Display/hide college 1 major 1 section 1 books
    $("#college2-major2-section2-btn").click(function() {
      $("#college2-major2-section2-books").toggle();
    });
    // Display/hide college 1 major 1 section 2 books
    $("#college2-major2-section2-btn").click(function() {
      $("#college2-major2-section2-books").toggle();
    });
    // Display/hide college 1 major 2 sections
    $("#college2-major2-btn").click(function() {
      $("#college2-major2-sections").toggle();
    });
    // Display/hide college 1 major 2 section 1 books
    $("#college2-major2-section1-btn").click(function() {
      $("#college2-major2-section1-books").toggle();
    });
    // Display/hide college 1 major 2 section 2 books
    $("#college2-major2-section2-btn").click(function() {
      $("#college2-major2-section2-books").toggle();
    });
    // Display book info on book button click
    $(".btn-warning").click(function() {
      var title = $(this).text();
      var author = $(this).parent().prev().text();
      var summary = $(this).parent().prev().prev().text();
      $("#book-title").text(title);
      $("#book-author").text(author);
      $("#book-summary").text(summary);
      $("#book-info").show();
    });
  });
</script>
